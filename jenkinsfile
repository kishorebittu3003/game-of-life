pipeline {
    agent any
    stages
    {
        stage ('git clone game of life') 
                steps
                {
                git url: 'https://github.com/kishorebittu3003/game-of-life.git',
                  branch: 'game'}
                }
       { stage ('mvn package')
            {
            steps
            {
                script{
                    sh 'mvn package'
                      }
            }
            }
        }
        }
        